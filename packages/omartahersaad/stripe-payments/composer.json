{
    "name": "omartahersaad/stripe-payments",
    "description": "Laravel Package for Integrating with Stripe Gateway for making payments",
    "authors": [
        {
            "name": "Omar Taher Saad",
            "email": "omartahersaad@outlook.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "illuminate/console": "^10.0",
        "illuminate/contracts": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/http": "^10.0",
        "illuminate/log": "^10.0",
        "illuminate/notifications": "^10.0",
        "illuminate/pagination": "^10.0",
        "illuminate/routing": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/view": "^10.0",
        "symfony/http-kernel": "^6.0",
        "stripe/stripe-php": "^12.1",
        "nesbot/carbon": "^2.69"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3"
    },
    "autoload": {
        "psr-4": {
            "OmarTaherSaad\\StripePayments\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OmarTaherSaad\\StripePayments\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OmarTaherSaad\\StripePayments\\StripePaymentsServiceProvider"
            ]
        }
    }
}
